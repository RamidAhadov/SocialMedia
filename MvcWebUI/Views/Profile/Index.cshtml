    <main>
        <div class="main-section">
            <div>
                <div class="cover-photo">
                    <img src="https://res.cloudinary.com/do2bvloo2/image/upload/v1700680908/a0osrzpceox4zy0auwr9.jpg">
                </div>
            </div>
            <div class="profile-choices">
                <img id="mainProfilePhoto" class="main-profile-photo">
                <div class="profile-choices-full-name" id="profileChoicesFullName">
                    <!-- User full name-->
                </div>
                <div class="profile-choices-choices" id="profileChoices">
                    
                </div>
                <div class="profile-settings" id="profileSettings">
                    
                </div>
            </div>
            <button id="executeGetPosts">Execute!</button>
            <div class="posts" id="postsContainer">
                <div class="post-list">
                    
                </div>
            </div>
        </div>
    </main>
